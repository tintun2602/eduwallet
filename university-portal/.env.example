# University Portal Environment Configuration
# Copy this file to .env and fill in your actual values

# University private key for blockchain operations
# This should be the private key of the university wallet that has permission to register students
REACT_APP_UNIVERSITY_PRIVATE_KEY=0xYourUniversityPrivateKeyHere

# Optional: Override default blockchain network URL
# REACT_APP_NETWORK_URL=http://127.0.0.1:8545

# Optional: Override default contract address
# REACT_APP_REGISTER_ADDRESS=0xDe09E74d4888Bc4e65F589e8c13Bce9F71DdF4c7
